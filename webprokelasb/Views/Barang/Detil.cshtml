@model webprokelasb.Entitas.Barang
<h2>Detil Barang</h2>
@if (Model != null)
{
<div>
    <div class="form-group">
        <div>
            <h4> Kode Kategori :<strong>@Model.KodeBarang</strong></h4>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-9">
            <h4> Nama Barang :<strong> @Model.NamaBarang</strong></h4>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-9">
            <h4> Harga :<strong> @Model.harga</strong></h4>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-9">
            <h4> Stok :<strong> @Model.Stok</strong></h4>
        </div>
    </div>
</div>
}
else
{
    <h2>Data Kategori Tidak Ditemukan</h2>
}
